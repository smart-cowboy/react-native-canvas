{
  "name": "react-native-canvas",
  "version": "0.1.10",
  "main": "dist/Canvas.js",
  "scripts": {
    "build": "babel src --out-dir dist --copy-files && node bundle-html.js ./dist/index.html",
    "build-to-example": "babel src --out-dir examples/node_modules/react-native-canvas/dist --copy-files --source-maps && node bundle-html.js ./dist/index.html && cp dist/index.html.js examples/node_modules/react-native-canvas/dist/index.html.js",
    "prepare": "yarn build"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react-native": "^2.1.0",
    "eslint": "^4.3.0",
    "eslint-config-fbjs-opensource": "^1.0.0",
    "prettier": "^1.5.3"
  },
  "dependencies": {
    "canvas-5-polyfill": "^0.1.5",
    "parse5": "^3.0.2"
  },
  "repository": "https://github.com/iddan/react-native-canvas"
}
